{
    "$schema": "https://turborepo.com/schema.json",
    "ui": "tui",
    "tasks": {
        "topo": {
            "dependsOn": [
                "^topo"
            ]
        },
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                ".cache/tsbuildinfo.json",
                "dist/**"
            ]
        },
        "dev": {
            "cache": false,
            "persistent": true
        },
        "format": {
            "outputs": [
                ".cache/.prettiercache"
            ],
            "outputLogs": "new-only"
        },
        "lint": {
            "dependsOn": [
                "^topo",
                "^build"
            ],
            "outputs": [
                ".cache/.eslintcache"
            ]
        },
        "typecheck": {
            "dependsOn": [
                "^topo",
                "^build"
            ],
            "outputs": [
                ".cache/tsbuildinfo.json"
            ]
        },
        "clean": {
            "cache": false
        },
        "//#clean": {
            "cache": false
        }
    },
    "remoteCache": {
        "enabled": true,
        "signature": false
    }
}